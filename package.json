{
  "name": "express-stack-lab",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "browser": "browser-sync start --config=bs-config.js",
    "server": "nodemon --exec babel-node -r ./node_modules/dotenv-safe/config ./server",
    "server:prod": "node -r dotenv-safe/config ./dist/server",
    "dev": "NODE_ENV=development run-p server browser",
    "build": "webpack --mode production",
    "prod": "NODE_ENV=production npm-run-all build server:prod",
    "lint": "eslint ./"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bootstrap-icons": "^1.0.0-alpha2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv-safe": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^3.1.0",
    "express-jwt": "^6.0.0",
    "helmet": "^3.21.2",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.9",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.27.10",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "nodemonConfig": {
    "watch": [
      "*"
    ],
    "ext": "css,hbs,html,js,json"
  }
}
